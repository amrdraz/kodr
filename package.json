{
  "name": "Kodr",
  "version": "0.5.11",
  "repository": {
    "type": "git",
    "url": "https://github.com/amrdraz/kodr.git"
  },
  "main": "prod-server.js",
  "scripts": {
    "start": "nodemon server.js",
    "test": "node_modules/.bin/mocha test/server -R spec --ui bdd"
  },
  "browserify-global-shim": {
    "jquery": "$"
  },
  "browserify": {
    "transform": [
      "browserify-global-shim"
    ]
  },
  "config": {
    "blanket": {
      "pattern": "back",
      "data-cover-never": [
        "node_modules",
        "test",
        "app",
        "data",
        ".tmp",
        "api"
      ]
    }
  },
  "dependencies": {
    "bcrypt": "^0.8.5",
    "bluebird": "^2.3.0",
    "body-parser": "~1.0.0",
    "browserify-global-shim": "^1.0.0",
    "captain-hook": "0.0.3",
    "compression": "^1.3.0",
    "connect-flash": "~0.1.1",
    "connect-redis": "^2.1.0",
    "cookie-parser": "~1.0.0",
    "cors": "^2.7.1",
    "dockerode": "^2.0.3",
    "eventemitter2": "^0.4.14",
    "express": "~4.0.0",
    "express-session": "~1.0.0",
    "java-code-runner": "^0.4.2",
    "jshint-stylish": "~0.1.5",
    "lodash": "^3.10.1",
    "memory-streams": "0.0.3",
    "method-override": "~1.0.0",
    "mongoose": "^4.1.6",
    "mongoose-relationship": "^0.1.4",
    "morgan": "^1.0.1",
    "multer": "^0.1.4",
    "nodemailer": "^1.2.1",
    "nodemailer-html-to-text": "^1.0.0",
    "nodemailer-smtp-pool": "^0.1.8",
    "nodemailer-smtp-transport": "^0.1.12",
    "nodemailer-stub-transport": "^0.1.4",
    "passport": "~0.1.17",
    "passport-facebook": "~1.0.2",
    "passport-google-oauth": "~0.1.5",
    "passport-local": "~0.1.6",
    "passport-twitter": "~1.0.2",
    "random-password-generator": "0.0.3",
    "requirejs": "^2.1.14",
    "sillyname": "0.0.3",
    "socket.io": "^1.4.5",
    "socket.io-client": "^1.4.5",
    "socket.io-redis": "^1.0.0",
    "structured": "^0.1.2",
    "swig": "^1.4.2",
    "swig-email-templates": "^2.0.0"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "devDependencies": {
    "blanket": "^1.1.7",
    "chai": "^3.2.0",
    "mocha": "^2.3.2",
    "sinon": "^1.10.3",
    "superagent": "^0.18.2",
    "supertest": "^0.13.0",
    "supertest-as-promised": "^1.0.0",
    "travis-cov": "^0.2.5"
  }
}
